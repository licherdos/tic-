#include <stdio.h>

int main()
{
    int year;
        printf("Introduce el año:");
        scanf("%d", &year);
        printf("\n");
        
    if(year % 4 == 0 && (year%100==0 && year%400!=0)){
        printf("El año es bisiesto");
    }    
    else{
        printf("El año no es bisiesto");
    }
    return 0;
}
