#include <stdio.h>

int main()
{
    int numero1; 
    int numero2;
   
    
    printf("Inserte numero 1:");
    scanf("%d", &numero1);
    printf("\n");
    
    printf("Inserte numero 2:");
    scanf("%d", &numero2);
    printf("\n");
    
    if(numero2 < numero1){
        int aux = numero1;
        numero1 = numero2;
        numero2 = aux;
    }
    int suma = 0;
    int i = numero1;
    while(i <= numero2){
        suma= i + suma;
        i = i + 1;
    }
    printf("Resultado final es: %d", suma);
    
